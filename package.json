{
  "name": "greek-in-tech",
  "version": "1.0.0",
  "description": "Learn which words used on modern computing and software engineering, come straight from the greek language/mythology.",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest test/*",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "xo **/*.js !lib/*.js",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "postcodecover": "npm run test:cover && codecov"
  },
  "files": [
    "lib",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/greek-in-tech"
  },
  "keywords": [
    "greek",
    "words",
    "tech",
    "names",
    "list",
    "data",
    "random"
  ],
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsevdos/greek-in-tech/issues"
  },
  "homepage": "http://greekintech.com",
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-plugin-istanbul": "4.1.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "jest": "^23.6.0",
    "rimraf": "2.6.2",
    "xo": "0.21.1"
  },
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "xo": {
    "space": 2,
    "envs": [
      "node"
    ]
  }
}
